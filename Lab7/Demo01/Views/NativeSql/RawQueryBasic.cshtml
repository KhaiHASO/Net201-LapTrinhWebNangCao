@model IEnumerable<Demo01.Models.Student>

@{
    ViewData["Title"] = "Raw Query Basic";
}

<h1>FromSqlRaw Demo</h1>
<p>Query: <code>SELECT * FROM Students WHERE Age > {0}</code> (Age > 18)</p>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Major</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.Age</td>
                <td>@item.Major</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Index" class="btn btn-secondary">Back to List</a>
